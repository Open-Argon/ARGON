const x = 10
const y = 10
var stopped = ''

sub genGrid(x, y) [
  const board = []
  var i = 0
  while (i<-x) [
    append(board, [])
    var j = 0
    while (j<-y) [
      append(board[i], 'O')
      j++
    ]
    i++
  ]
  return board
]

sub showBoard(board, x, y) [
  var i = 0
  while (i<-x) [
    log(join(' ', board[i]))
    i++
  ]
]

showBoard(genGrid(x, y), x, y)